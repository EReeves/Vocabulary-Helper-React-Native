{
    "compilerOptions": {
        "target": "es2015",
        "jsx": "react",
        "noEmit": false,
        "moduleResolution": "node",
        "noResolve": false,
        "noImplicitAny": false,
        "removeComments": true,
        "sourceMap": true,
        "rootDir": "./src",
        "outDir": "jsbin",
        "allowJs": true,
    },
    "exclude": [
        "node_modules/**/*",
        "AppData"
    ],
    "include": [
        "src/**/*"
    ]
}